generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Operador {
  id        Int      @id @default(autoincrement())
  nome      String   @unique
  senha     String
  ativo     Boolean  @default(true)
  criadoEm  DateTime @default(now())
}